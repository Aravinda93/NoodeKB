extends layout

block content
  br
  h1 #{title}
  br
  ul.list-group
    each article, i in articles
      li.list-group-item
        a(href = "/articles/"+article._id): h4 #{article.title}
        <br>
        h5 Author is: #{article.author}
        <br>
        h6 Intro: #{article.body}
        
